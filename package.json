{
  "name": "ecommerce-analytics-dashboard",
  "version": "1.0.0",
  "description": "E-commerce Analytics Dashboard - MERN Stack",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
